<p id="notice"><%= notice %></p>

<h1>Listing Posts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <div class="grid-container">
  <% @posts.each do |post| %>
    <div class="grid-item" style="width:200px;height:200px;background-image:url('<%= post.grid_image_url %>');background-size:contain;"><%= link_to 'hi', post %></div>  
  <% end %>    
  </div>


</table>

<br>

